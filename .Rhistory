# Function to create a triad
create_triad <- function() {
triad <- matrix(0, nrow = 3, ncol = 3)
triad[1, 2] <- triad[2, 3] <- triad[3, 1] <- 1
triad
}
# Set the number of triads
num_triads <- 100
# Create a network with 100 triads
triad_network <- as.sociomatrix(replicate(num_triads, create_triad()))
# Plot the triad network
plot(triad_network, main = "Network of 100 Triads")
